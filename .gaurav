     	echo "not in emacs"
else
	export PATH=$PATH:/usr/local/bin     
fi

shopt -s nocaseglob

export HISTSIZE=5000

export EDITOR="emacs -nw"

export PATH=$PATH:~/learning/udemy/kubernetes/softwares/bin